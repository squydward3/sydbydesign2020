          +ingredients ('Kickstarter', 'Product designer, Creator Tools', 'current')
          +ingredients ('SelfMade', 'Product designer', '2019-2020')
          +ingredients ('Abacus by Emburse', 'Product designer', '2018-2019')
          +ingredients ('Havas', 'Frontend developer freelancer', '2017')
          +ingredients ('Warner Music Group', 'Web intern', '2016')

          .row
            h3.col-sm-3.padding-top64 EDUCATION
